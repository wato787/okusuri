{
  "name": "okusuri",
  "version": "1.0.0",
  "private": true,
  "description": "Okusuri - お薬管理アプリケーション",
  "scripts": {
    "dev:frontend": "cd frontend && pnpm dev",
    "dev:v2": "cd okusuri-v2 && pnpm dev",
    "build:frontend": "cd frontend && pnpm build",
    "build:v2": "cd okusuri-v2 && pnpm build",
    "lint:frontend": "cd frontend && pnpm lint",
    "lint:v2": "cd okusuri-v2 && pnpm lint",
    "lint:fix:frontend": "cd frontend && pnpm lint:fix",
    "lint:fix:v2": "cd okusuri-v2 && pnpm lint:fix",
    "start:frontend": "cd frontend && pnpm start",
    "preview:v2": "cd okusuri-v2 && pnpm preview"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
